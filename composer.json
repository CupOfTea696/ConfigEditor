{
    "name": "cupoftea/config-editor",
    "description": "Programatically edit array-based php configuration files.",
    "keywords": [
        "config",
        "configuration",
        "editor",
        "php",
        "array"
    ],
    "homepage": "https://github.com/CupOfTea696/ConfigEditor",
    "license": "MIT",
    "authors": [
        {
            "name": "Frankie Wittevrongel",
            "email": "frankie@cupoftea.io"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "cupoftea/package": "^1.4",
        "illuminate/support": "^8.22",
        "nikic/php-parser": "^4.10"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CupOfTea\\Config\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/CupOfTea696/ConfigEditor/issues",
        "source": "https://github.com/CupOfTea696/ConfigEditor"
    }
}
